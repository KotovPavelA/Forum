@model MessagesViewModel

@foreach (Message item in Model.Messages)
{
<blockquote class="blockquote">
    <p class="mb-0">@item.Text</p>
    <footer class="blockquote-footer">@item.User.Nickname</footer>
    <p align="right">@item.Date</p>
    <p align="right">
        Понравилось: @item.Likes
    </p>

</blockquote>

}
